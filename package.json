{
  "name": "@luolapeikko/ts-common",
  "version": "0.0.4",
  "description": "Typescript common utils and types",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "doc": "typedoc",
    "build": "tsup src/index.ts --sourcemap --format cjs,esm --dts --clean",
    "prepublishOnly": "pnpm run build",
    "lint": "eslint src",
    "test": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luolapeikko/ts-common.git"
  },
  "keywords": [
    "typescript",
    "common",
    "interface",
    "type"
  ],
  "author": "mharj",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/luolapeikko/ts-common/issues"
  },
  "homepage": "https://github.com/luolapeikko/ts-common#readme",
  "devDependencies": {
    "@types/chai": "^4.3.16",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.12.12",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "chai": "^5.1.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-deprecation": "^2.0.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-sonarjs": "^0.25.1",
    "mocha": "^10.4.0",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.2",
    "type-testing": "^0.2.0",
    "typedoc": "^0.25.13",
    "typescript": "^5.4.5"
  }
}
