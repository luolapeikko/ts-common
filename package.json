{
  "name": "@luolapeikko/ts-common",
  "version": "0.1.3",
  "description": "Typescript common utils and types",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "doc": "typedoc",
    "build": "tsup src/index.ts --sourcemap --format cjs,esm --dts --clean",
    "prepublishOnly": "pnpm run build",
    "lint": "eslint src",
    "test": "mocha",
    "validate": "tsc --noEmit --project tsconfig.test.json"
  },
  "mocha": {
    "exit": true,
    "extension": [
      "ts",
      "js"
    ],
    "recursive": true,
    "enable-source-maps": true,
    "require": [
      "ts-node/register"
    ],
    "loader": "ts-node/esm",
    "reporters": [
      "spec",
      "mocha-junit-reporter"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luolapeikko/ts-common.git"
  },
  "keywords": [
    "typescript",
    "common",
    "interface",
    "type"
  ],
  "author": "mharj",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/luolapeikko/ts-common/issues"
  },
  "homepage": "https://github.com/luolapeikko/ts-common#readme",
  "devDependencies": {
    "@stylistic/eslint-plugin": "^2.6.2",
    "@types/chai": "^4.3.17",
    "@types/mocha": "^10.0.7",
    "@types/node": "^20.14.15",
    "@typescript-eslint/eslint-plugin": "^8.1.0",
    "@typescript-eslint/parser": "^8.1.0",
    "chai": "^5.1.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-deprecation": "^3.0.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-sonarjs": "^0.25.1",
    "mocha": "^10.7.3",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "tsup": "^8.2.4",
    "type-testing": "^0.2.0",
    "typedoc": "^0.26.5",
    "typescript": "^5.5.4"
  },
  "packageManager": "pnpm@9.6.0+sha256.dae0f7e822c56b20979bb5965e3b73b8bdabb6b8b8ef121da6d857508599ca35"
}